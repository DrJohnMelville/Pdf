<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>

        <Title>Melville.Parsing</Title>
        <PackageDescription>Low level parsing helpers used by multiple PDF sub-format parsers/</PackageDescription>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Melville.Generators.INPC" Version="0.5.42" />
      <PackageReference Include="System.IO.Pipelines" Version="6.0.3" />
      <PackageReference Include="Melville.Hacks" Version="0.5.42" />
    </ItemGroup>

</Project>
