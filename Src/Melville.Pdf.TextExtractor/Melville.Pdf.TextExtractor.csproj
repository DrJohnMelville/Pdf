<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <Title>Melville.Pdf.ImageExtractor</Title>
	  <PackageDescription>
		  This is a renderer for Melville.PDF that extracts all of the text from a PDF page.
	  </PackageDescription>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>    
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Melville.Pdf.Model\Melville.Pdf.Model.csproj" />
    <ProjectReference Include="..\Melville.AsyncAnalyzer\Melville.AsyncAnalyzer.csproj"
                      PrivateAssets="all">
	    <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
	    <OutputItemType>Analyzer</OutputItemType>
    </ProjectReference>
  </ItemGroup>

</Project>
