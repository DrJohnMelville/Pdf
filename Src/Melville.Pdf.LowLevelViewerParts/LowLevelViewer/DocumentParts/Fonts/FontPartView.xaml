<UserControl x:Class="Melville.Pdf.LowLevelViewerParts.LowLevelViewer.DocumentParts.Fonts.FontPartView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewFrames="clr-namespace:Melville.MVVM.Wpf.ViewFrames;assembly=Melville.MVVM.Wpf"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl>
        <TabItem Header="Generic">
            <viewFrames:ViewFrame Content="{Binding GenericFont}"/>
        </TabItem>
        <TabItem Header="Specific">
            <viewFrames:ViewFrame Content="{Binding SpecificFont}"/>
         </TabItem>
    </TabControl>
</UserControl>
